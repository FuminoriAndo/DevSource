<Application
    x:Class="CKSI1010.App" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:ignore="http://www.galasoft.ch/ignore" xmlns:l="clr-namespace:CKSI1010"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" DispatcherUnhandledException="OnDispatcherUnhandledException" StartupUri="/Base/View/BaseWindow.xaml"
    mc:Ignorable="d ignore" Startup="OnStartup">

    <Application.Resources>

        <ResourceDictionary>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Theme/style.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <l:ViewModelLocator x:Key="Locator" d:IsDataSource="True" />
        </ResourceDictionary>
    </Application.Resources>

</Application>
